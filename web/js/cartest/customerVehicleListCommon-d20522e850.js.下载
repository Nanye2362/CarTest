function maintainHistory(e,t){$(".modal-body").html("");var n=getRootPath()+"/maintain/"+e+"/toMaintainHistoryPopup.do?maintainType=GD&maintainType=LPD&maintainType=LSD&maintainType=XCD&carNo="+t;$("#historyModal").modal({remote:n,show:!0})}$(function(){if($("#editBtn").click(function(e){if(1!=parseInt($('input[name="chk"]:checked').length))return Messenger({extraClasses:"messenger-fixed messenger-on-top"}).post({message:"请选择一条数据.",type:"error",hideAfter:3}),!1;$('input[name="chk"]:checked').each(function(){var e=$(this).parent().find("input[name=idCustomer]").val(),t=$(this).parent().find("input[name=idCar]").val();return window.location.href=getRootPath()+"/customerCar/getCustomerCar/"+e+"/"+t+".do",!1})}),$("#deleteBtn").click(function(){if(0==parseInt($('input[name="chk"]:checked').length))return Messenger({extraClasses:"messenger-fixed messenger-on-top"}).post({message:"请选择要删除的数据.",type:"error",hideAfter:3}),!1;confirmMessageDialog("是否删除选中的数据?",function(){$('input[name="chk"]:checked').each(function(){var e=$(this).parent().find("input[name=idCustomer]").val(),t=$(this).parent().find("input[name=idCar]").val(),n=getRootPath()+"/customerCar/deleteCustomerCar/"+e+"/"+t+".do";$(this);ajaxGet("",n,"删除成功",function(e){"fail"!=e.status&&searchList()})})})}),$("#searchBtn").click(function(){searchList()}),$("#keyWord").keydown(function(e){13==e.keyCode&&searchList()}),$("#exportBtn").click(function(){window.location.href=getRootPath()+"/customerCar/downloadCustomerCar.do?carType="+$("#carType").val()}),location.hash){var e=location.hash.replace("#","");$("#keyWord").val(e),searchList()}$("#callBtn").click(function(){var e=$("input[name='chk']:checked"),t="";if(1!=e.length)return t="未选择记录，或者选择了多条记录。只能对单客户进行呼叫。",void Messenger().post({message:t,type:"error",hideAfter:3});var n=$(e.get(0)).attr("data-phoneNum");if(!n)return t="选中的客户暂无联系方式，无法进行呼叫。",void Messenger().post({message:t,type:"error",hideAfter:3});phoneCall(n)})});
