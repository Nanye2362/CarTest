function PageConsole(a,e,t,r,i,n,s,g){var l=this;l.element=g,l.render=g?g.find(a):jQuery(a),l.searchTaget=g?g.find(e):jQuery(e),l.curPage=""==t?0:parseInt(t),l.pageSize=""==r?0:parseInt(r),l.totalPages=""==i?0:parseInt(i),l.totalRecords=""==n?0:parseInt(n),l.width=s,l.mainTable=jQuery(""),l.labTotalPages=jQuery(""),l.labTotalRecords=jQuery(""),l.labFirst=jQuery(""),l.labPre=jQuery(""),l.laNext=jQuery(""),l.labLast=jQuery(""),l.jumpInput=jQuery(""),l.imgGo=jQuery(""),l.pageIndex=jQuery(""),l.pageTaget=jQuery(""),l.goFirst=function(){l.pageIndex.val(1),l.pageTaget.val("pageBar"),l.searchTaget.trigger("click")},l.goPre=function(){var a=l.curPage-1;a<=0&&(a=1),l.pageIndex.val(a),l.pageTaget.val("pageBar"),l.searchTaget.trigger("click")},l.goNext=function(){var a=l.curPage+1;a>l.totalPages&&(a=l.totalPages),l.pageIndex.val(a),l.pageTaget.val("pageBar"),l.searchTaget.trigger("click")},l.goLast=function(){l.pageIndex.val(l.totalPages),l.pageTaget.val("pageBar"),l.searchTaget.trigger("click")},l.goPage=function(a){return"上一页"!=a&&"下一页"!=a&&(/[\.]{1,}/.test(a)?(l.jumpInput.val(this.curPage),void l.jumpInput.focus()):isNaN(a)?(l.jumpInput.val(this.curPage),void l.jumpInput.focus()):(a<=0&&(a=1),a>l.totalPages&&(a=l.totalPages),l.pageIndex.val(a),l.pageTaget.val("pageBar"),void l.searchTaget.trigger("click")))},l.init=function(){var a="";if(a+='<div class="main-pagers text-center"><div style="display:inline-block;">',a+='<span disabled="disabled" class="main-pagers-margin">共<span id="labTotalRecords"  disabled="disabled"></span>条记录</span>',a+='<a  id="labPre" class="func-btn" href="javascript:void(0);"  >上一页</a>',l.curPage<=4)if(l.totalPages>4){for(e=1;e<=4;e++)l.curPage==e?a+='<a href="javascript:void(0);" class="active"   >'+e+"</a>":a+='<a href="javascript:void(0);"  >'+e+"</a>";a+='<a   href="javascript:void(0);"  >...</a>',a+='<a   href="javascript:void(0);"  >'+l.totalPages+"</a>"}else for(e=1;e<=l.totalPages;e++)l.curPage==e?a+='<a  href="javascript:void();"  class="active"  >'+e+"</a>":a+='<a  href="javascript:void();"  >'+e+"</a>";else if(l.curPage>=l.totalPages-3)if(l.totalPages>3){a+='<a  href="javascript:void(0);"  >1</a>',a+='<a   href="javascript:void(0);"  >...</a>';for(e=l.totalPages-3;e<=l.totalPages;e++)l.curPage==e?a+='<a href="javascript:void(0);" class="active"   >'+e+"</a>":a+='<a href="javascript:void(0);"  >'+e+"</a>"}else for(var e=1;e<=3;e++)l.curPage==e?a+='<a  href="javascript:void(0);"  class="active"   >'+e+"</a>":a+='<a  href="javascript:void(0);"  >'+e+"</a>";else a+='<a  href="javascript:void(0);"  >1</a>',a+="...",a+='<a  href="javascript:void(0);"  >'+(l.curPage-1)+"</a>",a+='<a  href="javascript:void(0);"  class="active"   >'+l.curPage+"</a>",a+='<a  href="javascript:void(0);"  >'+(l.curPage+1)+"</a>",a+="...",a+='<a  href="javascript:void(0);"  >'+l.totalPages+"</a>";return a+="</ul>",a+='<a class="func-btn main-pagers-margin" id="laNext" href="javascript:void(0);"  >下一页</a>',a+='<span >共<span id="labTotalPages"></span>页&nbsp;</span>',a+='<span>到第&nbsp;<input type="text" id="jumpInput" class="" name="jumpInput" size="3" value="0">&nbsp;页</span>',a+='&nbsp;<a id="imgGo" class="func-btn" style="cursor: hand">前往</a>',a+='<input type="hidden" id="pageIndex" name="pageIndex" />',a+='<input type="hidden" id="pageTaget" name="pageTaget" />',a+="</div></div>",l.render.append(a),l.mainTable=l.render.find("#mainTable"),l.labTotalPages=l.render.find("#labTotalPages"),l.labTotalRecords=l.render.find("#labTotalRecords"),l.labFirst=l.render.find("#labFirst"),l.labPre=l.render.find("#labPre"),l.laNext=l.render.find("#laNext"),l.labLast=l.render.find("#labLast"),l.jumpInput=l.render.find("#jumpInput"),l.imgGo=l.render.find("#imgGo"),l.aHref=l.render.find("a"),l.pageIndex=l.render.find("#pageIndex"),l.pageTaget=l.render.find("#pageTaget"),l.mainTable.attr("width",l.width),l.labTotalPages.html(l.totalPages),l.labTotalRecords.html(l.totalRecords),l.jumpInput.val(l.curPage),l.pageIndex.val(l.curPage),l.pageTaget.val("searchBtn"),l.labFirst.bind("click",function(){l.goFirst()}),l.labPre.bind("click",function(){l.goPre()}),l.laNext.bind("click",function(){l.goNext()}),l.labLast.bind("click",function(){l.goLast()}),l.imgGo.bind("click",function(){l.goPage(l.jumpInput.val())}),l.aHref.bind("click",function(){l.goPage(this.innerHTML)}),l.jumpInput.keydown(function(a){13==a.which&&l.goPage(l.jumpInput.val())}),l},l.init()}
